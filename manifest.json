{
  "manifest_version": 2,

  "name": "Unbias-me",
  "description": "This extension will replace images with robot images",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "unbias hired.com"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://hired.com/list/", "https://hired.com/list", "https://hired.com/list?page=*"],
      "js": ["jquery.js", "list.js"]
    },
    {
      "matches": ["https://hired.com/list/*?position=*"],
      "js": ["jquery.js", "individual.js"]
    }
  ],
  "background": {
    "scripts": ["list.js"],
    "persistent": false
  }
}
